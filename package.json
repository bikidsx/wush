{
  "name": "wush",
  "version": "1.0.0",
  "description": "AI-powered Git workflow CLI",
  "type": "module",
  "bin": {
    "wush": "./bin/wush"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "start": "bun run src/index.ts",
    "type-check": "tsc --noEmit",
    "link": "bun link"
  },
  "keywords": ["git", "ai", "cli", "commit", "pr", "security"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@google/generative-ai": "^0.21.0",
    "@octokit/rest": "^21.0.2",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "groq-sdk": "^0.7.0",
    "inquirer": "^12.0.1",
    "ollama": "^0.5.9",
    "openai": "^4.73.1",
    "ora": "^8.1.1",
    "parse-diff": "^0.11.1",
    "semver": "^7.6.3",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/semver": "^7.5.8",
    "bun-types": "^1.1.34",
    "typescript": "^5.7.2"
  }
}
